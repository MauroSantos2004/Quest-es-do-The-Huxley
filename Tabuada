numero = int(input())
a = numero * 1
b = numero * 2
c = numero * 3
d = numero * 4
e = numero * 5
f = numero * 6
g = numero * 7
h = numero * 8
i = numero * 9
print(f'{numero} X 1 = {a}')
print(f'{numero} X 2 = {b}')
print(f'{numero} X 3 = {c}')
print(f'{numero} X 4 = {d}')
print(f'{numero} X 5 = {e}')
print(f'{numero} X 6 = {f}')
print(f'{numero} X 7 = {g}')
print(f'{numero} X 8 = {h}')
print(f'{numero} X 9 = {i}')
