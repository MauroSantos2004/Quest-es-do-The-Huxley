entradas = int(input())
for x in range(entradas):
    selecionar1 = int(input())
    selecionar2 = input()
    selecionar3 = input().upper()
    selecionar4 = input().upper()
    selecionar5 = input()
    selecionar6 = int(input())
    print(f'Idade: {selecionar1}')
    print(f'Nome: {selecionar2}')
    print(f'Sexo: {selecionar3}')
    print(f'Estado Civil: {selecionar4}')
    print(f'Numero de amigos: {selecionar5}')
    print(f'Numero de fotos: {selecionar6}\n')
